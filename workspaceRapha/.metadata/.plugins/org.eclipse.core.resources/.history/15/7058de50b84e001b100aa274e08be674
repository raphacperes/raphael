package variaveis;

public class TiposDados {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		
		String nome = "Rapha";
		int idade = 45;
		double altura = 1.75;
		
		
		System.out.println("Nome :" + nome);
		System.out.println("Idade :" + idade);
		System.out.println("Altura: " + altura);
		
		
	}

}
